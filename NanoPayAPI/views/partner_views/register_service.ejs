<html>
<h1>Register New Service</h1>
<%- include ('../partials/messages') %>
<form action="/partner/register_service" method="POST">
<div>
    <input
        type="name"
        name="name"
        placeholder="Enter Name"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
  
<div>
    <input
        type="cost_to_user"
        name="cost_to_user"
        placeholder="cost to user"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
</div>
<div>
    <input
        type="price_to_partner"
        name="price_to_partner"
        placeholder="price_to_partner"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
</div>
<div>
    <input
        type="time_to_consent"
        name="time_to_consent"
        placeholder="time_to_consent"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
</div>
<div>
    <input
        type="time_valid"
        name="time_valid"
        placeholder="time_valid"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
</div>

<div>
    <input
        type="keys"
        name="keys"
        placeholder="keys"
        value="<%= typeof name != 'undefined' ? name : '' %>"
    />
</div>


<button type="submit" class="btn btn-primary btn-block">Register</button>
</form>
</html>